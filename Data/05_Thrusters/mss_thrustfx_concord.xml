<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema"><!--Vanilla--><!-- LG -->
	<Definition xsi:type="MyObjectBuilder_ThrustDefinition"><!-- LargeBlockLargeHydrogenThrust -->
		<Id>
			<TypeId>Thrust</TypeId>
			<SubtypeId>mss_lg_thrust_concord_3x3</SubtypeId>
		</Id><!-- Normal Keen Properties 
    ...
    -->
		<ModExtensions>
			<ModComponents>
				<GameLogicComponent Name="MwiComponent"/>
			</ModComponents>
			<Group Name="ThrusterCompData">
				<Text Name="ID" Value="mss_lg_thrust_concord_3x3"/><!--REQUIRED! No specific value required Debugging  -->
				<Boolean Name="hideVanillaFlame" Value="true"/><!-- Hide Vanilla Flame-->
				<Decimal Name="ThrustRateGain" Value="0.06"/>
				<Decimal Name="ThrustRateLoss" Value="0.3"/>
				<Boolean Name="UseEmissive" Value="false"/><!-- Enable heatup/emissive material effect, off = more performant -->
				<Text Name="EmissiveMaterial" Value="heatup"/><!-- custom material for emissive that react to thrust -->
				<Decimal Name="EmissiveRateGain" Value="0.005"/>
				<Decimal Name="EmissiveRateLoss" Value="0.01"/>
				<Text Name="EmitterSetup" Value="
                    You can add comments like this and that it ends with -&gt; ;
                    
                    Particle : KWS2_ThrusterFlame_Large;
                    Type: SPRITE;
                    AtmoOnly: FALSE;
                    ThrustMin : 0.01;
                    ThrustMax : 2;
                    Size: 1;
                    Offset: 0;
                    OffsetScaling:0;
                    VelocityScalingFrom: 1;
                    VelocityScalingTo: 1;
                    Dummies : thruster_flame;
                    "/>
			</Group>
		</ModExtensions>
	</Definition>
	<Definition xsi:type="MyObjectBuilder_ThrustDefinition"><!-- LargeBlockSmallHydrogenThrust -->
		<Id>
			<TypeId>Thrust</TypeId>
			<SubtypeId>mss_lg_thrust_concord_1x1</SubtypeId>
		</Id><!-- Normal Keen Properties 
    ...
    -->
		<ModExtensions>
			<ModComponents>
				<GameLogicComponent Name="MwiComponent"/>
			</ModComponents>
			<Group Name="ThrusterCompData">
				<Text Name="ID" Value="mss_lg_thrust_concord_1x1"/><!--REQUIRED! No specific value required Debugging  -->
				<Boolean Name="hideVanillaFlame" Value="true"/><!-- Hide Vanilla Flame-->
				<Decimal Name="ThrustRateGain" Value="0.12"/>
				<Decimal Name="ThrustRateLoss" Value="0.3"/>
				<Boolean Name="UseEmissive" Value="true"/><!-- Enable heatup/emissive material effect, off = more performant -->
