                    Particle : KWS2_ThrusterFlameNoGlitch;
                    Type: SPRITE;
                    AtmoOnly: FALSE;
                    ThrustMin : 0.01;
                    ThrustMax : 1;
                    Size: 1.5;
                    Offset: -1;
                    OffsetScaling: 3;
                    VelocityScalingFrom: -1;
                    VelocityScalingTo: 2;
                    Dummies : none;
                    "/>
			</Group>
		</ModExtensions>
	</Definition>
</Definitions>